{
  "compilerOptions": {
    "jsx": "react",
    "module": "commonjs",
    "sourceMap": true,
    "target": "es5",
    "lib": [ "es6", "dom" ],
    "suppressImplicitAnyIndexErrors": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "strictNullChecks": false,
    "experimentalDecorators": true
  },
  "exclude": [
    "node_modules", "wwwroot"
  ]
}